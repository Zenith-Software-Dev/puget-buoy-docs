 clock."}],"identifier":"doc:\/\/com.zenithsoftware.Puget-Buoy-Beta\/documentation\/Puget_Buoy_Beta\/AutoQueCountdownClock","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"AutoQueCountdownClock"}],"url":"\/documentation\/puget_buoy_beta\/autoquecountdownclock"},"doc://com.zenithsoftware.Puget-Buoy-Beta/documentation/Puget_Buoy_Beta":{"role":"collection","title":"Puget Buoy Beta","abstract":[],"identifier":"doc:\/\/com.zenithsoftware.Puget-Buoy-Beta\/documentation\/Puget_Buoy_Beta","kind":"symbol","type":"topic","url":"\/documentation\/puget_buoy_beta"},"doc://com.zenithsoftware.Puget-Buoy-Beta/documentation/Puget_Buoy_Beta/AutoQueCountdownClock/View-Implementations":{"role":"collectionGroup","title":"View Implementations","abstract":[],"identifier":"doc:\/\/com.zenithsoftware.Puget-Buoy-Beta\/documentation\/Puget_Buoy_Beta\/AutoQueCountdownClock\/View-Implementations","kind":"article","type":"topic","url":"\/documentation\/puget_buoy_beta\/autoquecountdownclock\/view-implementations"}}}